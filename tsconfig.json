{
  "compilerOptions": {
    "alwaysStrict": true,
    "baseUrl": "./src",
    "declaration": true,
    "declarationDir": "./types",
    "esModuleInterop": true,
    "lib": ["dom","es6"],
    "module": "commonjs",
    "noEmitOnError": true,
    "noImplicitAny": true,
    "outDir": "./dist",
    "removeComments": true,
    "resolveJsonModule": true,
    "rootDir": "./src",
    "sourceMap": false,
    "strict": true,
    "target": "es5",
  },
}
